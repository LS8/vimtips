{
  "manifest_version": 2,

  "name": "Vim Tips - New Tab Replacement",
  "short_name": "Vim Tips",
  "description": "Practical Vim Tips shown on your new tab page.",
  "version": "0.2",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  }
}
